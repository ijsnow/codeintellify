{
  "extends": ["config:base"],
  "timezone": "America/Los_Angeles",
  "schedule": ["on the 1st through 7th day of the month"],
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": ["on the 1st through 7th day of the month"]
  },
  "packageRules": [
    {
      "depTypeList": ["dependencies"],
      "rangeStrategy": "widen"
    },
    {
      "depTypeList": ["devDependencies"],
      "rangeStrategy": "replace"
    }
  ],
  "prCreation": "not-pending"
}
